
- var pictures = ['Рисунки и картинки','Посмотреть все','Dark','Abstraction','Art Core','Color','Мемы','Vegan','Надписи','Для мужчин','Черепа','Dark'];
.container__circleCollections
  ul.circleCollections 
    - for ( var t = 0; t < pictures.length; t++ )
      - var number = (Math.random() * pictures.length).toFixed(0);
      li.circleCollections__item
        .circleCollections__collectionImage(style="background-image:url(images/collection"+ number +".jpg)")
        .circleCollections__text #{pictures[number]}
